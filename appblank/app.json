{
  "expo": {
    "name": "appblank",
    "slug": "appblank",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/splash.gif",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.gif",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/splash.gif",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/splash.gif"
    },
        "plugins": [
      "expo-splash-screen"
    ]
  }
}
